{
    "name": "Color Picker",
    "version": "0.0.1",
    "manifest_version": 3,
    "author": "Prit Suvagiya",
    "description": "Pick any color on the screen to get the HEX code!",
    "action": {
        "default_icon": "dropper.png",
        "default_title": "ColorPicker",
        "default_popup": "popup.html"
    },
    "icons": {
        "64": "dropper.png",
        "128": "dropper.png"
    },
    "permissions": ["tabs"]
}